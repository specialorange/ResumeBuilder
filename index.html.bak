  <script>
  function calcDate(date1,date2) {
    var diff = Math.floor(date1.getTime() - date2.getTime());
    var day = 1000 * 60 * 60 * 24;

    var days = Math.floor(diff/day);
    var monthsDirty = Math.floor(days/31);
    var yearsDirty = Math.floor(monthsDirty/12);

    var monthsClean = (monthsDirty == 1) ? '1 month' : monthsDirty % 12 + ' months';
    var yearsClean = (yearsDirty == 1) ? '1 year' : yearsDirty + ' years';

    var message = yearsClean + ' ' + monthsClean;

    return message
  }
  var today = new Date()

  // equivalent to 0 01 2013
  var past1 = new Date(2013,00,01)
  var past2 = new Date(2015,04,01)
  $('#job1').text( calcDate(today,past1) );
  $('#job1a').text( calcDate(today,past1) );
  $('#job2').text( calcDate(today,past2) );
  $('#job2a').text( calcDate(today,past2) );

